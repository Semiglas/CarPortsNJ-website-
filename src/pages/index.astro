  <script>
    import { fade } from "astro:transitions";
    import lightGallery from "lightgallery";
    import { gsap } from "gsap";

    // Plugins
    import lgThumbnail from "lightgallery/plugins/thumbnail";
    import lgZoom from "lightgallery/plugins/zoom";

    lightGallery(document.getElementById("lightgallery"), {
      plugins: [lgZoom, lgThumbnail],
      licenseKey: "your_license_key",
      speed: 500,
      mode: "lg-fade",
      // ... other settings
    });

    const expandableItems = document.querySelectorAll(".expandable-item");
    expandableItems.forEach((item) => {
      const buttonToExpand = item.querySelector(".expandable-svg");
      const description = item.querySelector(".expandable-item-description");
      const expandableTitle = item.querySelector(
        ".expandable-item-title-container"
      );
      expandableTitle.onclick = function (event) {
        const wasExpanded =
          description.getAttribute("data-expanded") === "true" ? true : false;

        document.querySelectorAll(".expandable-item").forEach((item) => {
          const otherDescription = item.querySelector(
            ".expandable-item-description"
          );
          otherDescription.setAttribute("data-expanded", "false");
          const otherButtonToExpand = item.querySelector(".expandable-svg");
          otherButtonToExpand.innerHTML = `<svg
				viewBox="0 0 24 24"
				width="100%"
				height="100%"
				fill="none"
				xmlns="http://www.w3.org/2000/svg"
				><g id="SVGRepo_bgCarrier" stroke-width="0"></g><g
				  id="SVGRepo_tracerCarrier"
				  stroke-linecap="round"
				  stroke-linejoin="round"></g><g id="SVGRepo_iconCarrier">
				  <path
					d="M13 3C13 2.44772 12.5523 2 12 2C11.4477 2 11 2.44772 11 3V11H3C2.44772 11 2 11.4477 2 12C2 12.5523 2.44772 13 3 13H11V21C11 21.5523 11.4477 22 12 22C12.5523 22 13 21.5523 13 21V13H21C21.5523 13 22 12.5523 22 12C22 11.4477 21.5523 11 21 11H13V3Z"
					fill="#0F0F0F"></path>
				</g></svg
			  >`;
          const otherExpandableTitle = item.querySelector(
            ".expandable-item-title-container"
          );
          otherExpandableTitle.querySelector(
            ".expandable-item-title"
          ).style.color = "initial";
          otherExpandableTitle.style.backgroundColor = "initial";
          otherExpandableTitle.style.color = "#1e0101";
          otherDescription.style.outline = "unset";
        });

        if (!wasExpanded) {
          description.setAttribute("data-expanded", "true");
          buttonToExpand.innerHTML = `<svg viewBox="0 0 24 24" width='100%' height='100%' fill="none" xmlns="http://www.w3.org/2000/svg"><g id="SVGRepo_bgCarrier" stroke-width="0"></g><g id="SVGRepo_tracerCarrier" stroke-linecap="round" stroke-linejoin="round"></g><g id="SVGRepo_iconCarrier"> <path d="M2 12C2 11.4477 2.44772 11 3 11H21C21.5523 11 22 11.4477 22 12C22 12.5523 21.5523 13 21 13H3C2.44772 13 2 12.5523 2 12Z" fill="#FFFFFF"></path> </g></svg>`;
          expandableTitle.style.backgroundColor = "#bc1f20";
          console.log(expandableTitle);
          expandableTitle.querySelector(".expandable-item-title").style.color =
            "white";
          expandableTitle.style.color = "white";
          description.style.outline = "solid 1px #bc1f20";
        }
      };
    });

    const width = document.querySelector(".width");

    const height = document.querySelector(".height");
    console.log(width, height);
    const squareFeet = document.querySelector(".square-feet");
    const priceRange = document.querySelector(".price-range");

    [width, height].forEach((e) => {
      e.addEventListener("input", recalculate);
    });

    function recalculate() {
      squareFeet.innerText = +width.value * +height.value;
      console.log(width);
      priceRange.innerText = "$" + +squareFeet.innerText * 40;
      if (squareFeet.innerText.length > 8 || priceRange.innerText.length > 8) {
        squareFeet.innerText = "NaN";
        priceRange.innerText = "NaN";
      }
    }

    document.addEventListener("DOMContentLoaded", () => {
      var menuList = document.querySelector("#menu-container .menu-list");
      function slideMenu() {
        var activeState = menuList.classList.contains("active");
        console.log(activeState);
        gsap.to(menuList, {
          duration: 0.4, // 400 milliseconds = 0.4 seconds
          right: activeState ? "0%" : "-100%",
          ease: "power1.out", // this is a basic easing function for a smooth animation
        });
        console.log("im sliding menu");
      }

      document
        .querySelector("#menu-wrapper")
        .addEventListener("click", function (event) {
          event.stopPropagation();
          document.querySelector("#hamburger-menu").classList.toggle("open");
          document
            .querySelector("#menu-container .menu-list")
            .classList.toggle("active");
          slideMenu();
          document.body.classList.toggle("overflow-hidden");
        });

      const menuItems = document.querySelectorAll("ul.menu-list.accordion a");
      console.log(menuItems);
      menuItems.forEach((e) => {
        e.addEventListener("click", function () {
          gsap.to(menuList, {
            duration: 0.4, // 400 milliseconds = 0.4 seconds
            right: "-100%",
            ease: "power1.out", // this is a basic easing function for a smooth animation
          });
          document.querySelector("#hamburger-menu").classList.toggle("open");
          document
            .querySelector("#menu-container .menu-list")
            .classList.toggle("active");
        });
      });
    });


  </script>
